pipeline {
    agent none // Desabilita a alocação de um nó e executará na máquina mestre
    
    stages {
        stage('Execute on Master') {
            agent any // Opcional: Se você quiser executar esta etapa em um nó escravo específico, defina o agente aqui
            steps {
                sh 'echo "Hello World1"'
                sh 'echo "Hello World2"'
            }
        }
    }
}